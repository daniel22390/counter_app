{
  "expo": {
    "name": "Counter",
    "slug": "counter_app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/counter.png",
    "splash": {
      "image": "./assets/splash_counter.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brainweb.counterapp"
    },
    "android": {
      "package": "com.brainweb.counterapp"
    }
  }
}
